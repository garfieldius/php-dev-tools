{
  "name": "grossberger-georg/php-dev-tools",
  "description": "Common tools for PHP projects",
  "type": "library",
  "license": "Apache-2.0",
  "config": {
    "bin-dir": "bin/",
    "plaform": {
      "php": "7.0.8"
    }
  },
  "authors": [
    {
      "name": "Georg GroÃŸberger",
      "email": "contact@grossberger-ge.org"
    }
  ],
  "require": {
    "php": ">=7.0",
    "phpunit/phpunit": ">=6.5",
    "friendsofphp/php-cs-fixer": "^2.10"
  },
  "require-dev": {
    "typo3/cms-core": ">=8.7",
    "typo3/cms-extbase": ">=8.7"
  },
  "autoload": {
    "psr-4": {
      "GrossbergerGeorg\\PHPDevTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GrossbergerGeorg\\PHPDevTools\\Tests\\": "tests/"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".install/"
    }
  }
}
